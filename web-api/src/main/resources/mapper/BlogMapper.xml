<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.weilai.mapper.BlogMapper">

    <select id="selectMyPage" resultType="com.weilai.response.BlogRecordResponse">
        select cb.*,cu.nickname,cu.avatar from cc_blog cb left join cc_user cu on cb.user_id = cu.id
            ${ew.customSqlSegment}
    </select>
</mapper>
